<Window x:Class="GithubDisplay.GettersOfObservable"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GithubDisplay"
        mc:Ignorable="d"
        Title="GettersOfObservable" Height="300" Width="300">
    <StackPanel>
        <ListBox ItemsSource="{Binding AllItems}" />
        <ListBox ItemsSource="{Binding FilteredItems}" />
        <Button Content="Add Item" Click="Button_Click"/>
    </StackPanel>
</Window>
